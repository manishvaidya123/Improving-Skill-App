<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".First">

    <Button
        android:id="@+id/button10"
        android:layout_width="135dp"
        android:layout_height="125dp"
        android:layout_alignTop="@+id/button9"
        android:layout_alignBottom="@+id/button9"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="44dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="-7dp"
        android:layout_toEndOf="@+id/button9"
        android:onClick="problem"
        android:text="Problem Solving "
        android:textStyle="bold" />

    <Button
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="86dp"
        android:layout_marginEnd="19dp"
        android:text="Logout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.913"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="118dp"
        android:layout_alignTop="@+id/button8"
        android:layout_alignEnd="@+id/button10"
        android:layout_alignBottom="@+id/button8"
        android:layout_marginStart="36dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="-8dp"
        android:layout_marginBottom="9dp"
        android:layout_toEndOf="@+id/button8"
        android:onClick="leadership"
        android:text="Leadership"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="131dp"
        android:layout_below="@+id/textView6"
        android:layout_alignParentStart="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="80dp"
        android:onClick="communication"
        android:text="Communication "
        android:textColorHighlight="#FFEB3B"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button9"
        android:layout_width="137dp"
        android:layout_height="121dp"
        android:layout_below="@+id/button8"
        android:layout_alignStart="@+id/button8"
        android:layout_alignEnd="@+id/button8"
        android:layout_marginStart="3dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="5dp"
        android:onClick="organisation"
        android:text="Organisation"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView6"
        style="@style/Widget.AppCompat.TextView.SpinnerItem"
        android:layout_width="292dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="81dp"
        android:layout_marginEnd="-6dp"
        android:layout_toStartOf="@+id/logout"
        android:text="Type Of Skill"
        android:textSize="40dp"
        android:textStyle="bold|italic" />


</RelativeLayout>